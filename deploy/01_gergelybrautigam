server {
    server_name gergelybrautigam.com;
    listen 9090;
    root /var/www/html/gergelybrautigam;
    location / {
    }
    location ~* \.(?:css|js)$ {
        access_log off;
        #expires 7d;
        add_header Cache-Control public;
        try_files $uri /$uri/ /$1/$2;
    }
    location /img/ {
        autoindex on;
    }
}
